CREATE NONCLUSTERED INDEX 
    IX_FSBeneficiaries_TreatmentsID ON dbo.FSBeneficiaries 
    ( 
     TreatmentsID 
    ) 
    INCLUDE ( ID , TotalMoney ) 
    WITH 
    ( 
     FILLFACTOR = 80 , 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
GO 


CREATE NONCLUSTERED INDEX 
    IX_FSBeneficiaries_TreatmentsID_UserBankAccountID ON dbo.FSBeneficiaries 
    ( 
     TreatmentsID , 
     UserBankAccountID 
    ) 
    WITH 
    ( 
     FILLFACTOR = 80 , 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
GO

