CREATE TABLE dbo.tb_Notification 
    (
     notf_ID BIGINT NOT NULL IDENTITY , 
     notf_appid INTEGER , 
     notf_appname NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     notf_appobjectid BIGINT , 
     notf_appobjectname NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     notf_type CHAR (10) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     notf_emailtype CHAR (10) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     notf_sender NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     notf_recipients NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     notf_subj NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     notf_body NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     notf_xsltfile NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     notf_creatingdate DATETIME (8) DEFAULT getdate() , 
     notf_senddate DATETIME (8) , 
     notf_failed_attempts TINYINT , 
     notf_status CHAR (1) COLLATE SQL_Latin1_General_CP1_CI_AS DEFAULT 'P' , 
     notf_exception_message NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS 
    )
    ON [PRIMARY]
    TEXTIMAGE_ON 'PRIMARY' 
GO 



EXEC sp_addextendedproperty 'MS_Description' , 'The number of failed attempts to send an e-mail or sms' , 'SCHEMA' , 'dbo' , 'TABLE' , 'tb_Notification' , 'COLUMN' , 'notf_failed_attempts' 
GO



EXEC sp_addextendedproperty 'MS_Description' , 'F -> Failed,
P -> Pending,
S -> Sent' , 'SCHEMA' , 'dbo' , 'TABLE' , 'tb_Notification' , 'COLUMN' , 'notf_status' 
GO



EXEC sp_addextendedproperty 'MS_Description' , 'If a mail or sms fails to send, the exception message is saved here' , 'SCHEMA' , 'dbo' , 'TABLE' , 'tb_Notification' , 'COLUMN' , 'notf_exception_message' 
GO

ALTER TABLE dbo.tb_Notification ADD CONSTRAINT PK_tb_Notification PRIMARY KEY CLUSTERED (notf_ID)
     WITH (STATISTICS_NORECOMPUTE = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     ALLOW_ROW_LOCKS = ON )
     ON [PRIMARY]
GO

USE Mawhiba7
GO 

