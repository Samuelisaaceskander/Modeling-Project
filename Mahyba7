CREATE TABLE dbo.Systems 
    (
     ID INTEGER NOT NULL IDENTITY , 
     Name NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     EnglishName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Details NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ShortName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     WebSiteURL NVARCHAR (2000) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Email NVARCHAR (350) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     DepartmentID INTEGER , 
     StartDate DATE NOT NULL , 
     EndDate DATE , 
     IsPeriodically BIT , 
     IsFullyAutomated BIT NOT NULL , 
     HasTransactions BIT NOT NULL , 
     HasScientificFields BIT NOT NULL , 
     TypeID SMALLINT NOT NULL , 
     SubtypeID SMALLINT , 
     OrganizationID INTEGER , 
     LogoURL NVARCHAR (350) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     LargeLogoURL NVARCHAR (350) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Created DATETIME (8) NOT NULL DEFAULT getdate() , 
     CreatedBy BIGINT NOT NULL , 
     Updated DATETIME (8) NOT NULL DEFAULT getdate() , 
     UpdatedBy BIGINT NOT NULL , 
     OldSystemID INTEGER , 
     OldName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     CertificateTypeID SMALLINT 
    )
    ON [PRIMARY]
GO 



