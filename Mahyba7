    IX_ONSC_ArbitratorAssignProjects_ProjectID_ID_AssignStatus ON dbo.ONSC_ArbitratorAssignProjects 
    ( 
     ProjectID , 
     ID , 
     AssignStatus 
    ) 
    WITH 
    ( 
     FILLFACTOR = 80 , 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
GO

