CREATE TABLE dbo.TestUsers 
    (
     ID SMALLINT NOT NULL IDENTITY , 
     UserID BIGINT NOT NULL , 
     Details NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Created DATETIME (8) NOT NULL DEFAULT getdate() , 
     CreatedBy BIGINT NOT NULL 
    )
    ON [PRIMARY]
GO 

    


CREATE UNIQUE NONCLUSTERED INDEX 
    IX_TestUsers_UserID ON dbo.TestUsers 
    ( 
     UserID 
    ) 
    WITH 
    ( 
     FILLFACTOR = 70 , 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
GO

ALTER TABLE dbo.TestUsers ADD CONSTRAINT PK_TestUsers PRIMARY KEY CLUSTERED (UserID)
     WITH (
    FILLFACTOR = 70 , STATISTICS_NORECOMPUTE = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     ALLOW_ROW_LOCKS = ON )
     ON [PRIMARY]
GO

