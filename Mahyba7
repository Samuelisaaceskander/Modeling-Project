     Rank INTEGER , 
     CreatedBy BIGINT , 
     ModifiedBy BIGINT , 
     DownloadCount INTEGER DEFAULT (0) , 
     Hits INTEGER DEFAULT (0) , 
     IsPublished BIT , 
     FileSize BIGINT , 
     FileExtension NVARCHAR (10) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     FileURL NVARCHAR (150) COLLATE SQL_Latin1_General_CP1_CI_AS , 
