     ImageURL2 NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     TransactionID BIGINT , 
     IsSend BIT , 
     SendDate SMALLDATETIME , 
     ProjectStatusID INTEGER , 
