CREATE TABLE dbo.UserExamCategories 
    (
     ID SMALLINT NOT NULL IDENTITY , 
     Title NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     Details NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Created DATETIME (8) NOT NULL DEFAULT getdate() , 
     MinDegree INTEGER , 
     MaxDegree INTEGER , 
     ExamTypeID SMALLINT 
    )
    ON [PRIMARY]
GO 

    


CREATE UNIQUE NONCLUSTERED INDEX 
    IX_UserExamCategories_UniqueTitle ON dbo.UserExamCategories 
    ( 
     Title 
    ) 
    WITH 
    ( 
     FILLFACTOR = 70 , 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
GO

ALTER TABLE dbo.UserExamCategories ADD CONSTRAINT PK_UserExamCategories PRIMARY KEY CLUSTERED (ID)
     WITH (
    FILLFACTOR = 70 , STATISTICS_NORECOMPUTE = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     ALLOW_ROW_LOCKS = ON )
     ON [PRIMARY]
GO

USE Mawhiba7
GO 

CREATE TABLE dbo.UserExams 
    (
     ID BIGINT NOT NULL IDENTITY , 
     UserID BIGINT NOT NULL , 
     CategoryID SMALLINT NOT NULL , 
     Title NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     Details NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Degree NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     PassDate DATETIME (8) , 
     ImageURL NVARCHAR (2000) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ReferenceLink NVARCHAR (2000) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Created DATETIME (8) NOT NULL DEFAULT getdate() , 
     CreatedBy BIGINT NOT NULL , 
     Updated DATETIME (8) , 
     UpdatedBy BIGINT , 
     UserExamBatchID SMALLINT 
    )
    ON [PRIMARY]
    TEXTIMAGE_ON 'PRIMARY' 
GO

ALTER TABLE dbo.UserExams ADD CONSTRAINT PK_UserExams PRIMARY KEY CLUSTERED (ID)
     WITH (
    FILLFACTOR = 70 , STATISTICS_NORECOMPUTE = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     ALLOW_ROW_LOCKS = ON )
     ON [PRIMARY]
GO

