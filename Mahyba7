    (
     name sysname (256) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     principal_id INTEGER NOT NULL , 
     diagram_id INTEGER NOT NULL IDENTITY , 
     version INTEGER , 
     definition VARBINARY (MAX) 
