CREATE TABLE dbo.UDBNationalProgramHistory 
    (
     ID INTEGER NOT NULL IDENTITY , 
     MyID INTEGER NOT NULL , 
     RegDate NVARCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserArabicFirstName NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserArabicFatherName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserArabicGrandFatherName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserArabicFamilyName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     BirthDate NVARCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     NationalID NVARCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserGender NVARCHAR (255) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     City NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Stage NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     School NVARCHAR (150) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SchoolType NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     HomePhone NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     StudentMobile NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     FatherMobile NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Tested NVARCHAR (255) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ProgramName NVARCHAR (255) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     InternationalNomination BIT NOT NULL , 
     UDBProgrammID INTEGER , 
     Participation BIT NOT NULL , 
     CompletedProgram BIT NOT NULL , 
     IsExcuse BIT , 
     ExcuseReason NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ProgramDegree NVARCHAR (255) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Verbal NVARCHAR (255) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Quantitative NVARCHAR (255) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     QyiasTotal NVARCHAR (255) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     EnglishTest NVARCHAR (255) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserEmailAddress NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Area NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserID BIGINT , 
     UDBUniversityID INTEGER , 
     IsValidID BIT DEFAULT (1) , 
     ServiceUserID BIGINT , 
     [Current Stutus] INTEGER 
    )
    ON [PRIMARY]
GO

