CREATE TABLE dbo.ExceptionsLog 
    (
     ID BIGINT NOT NULL IDENTITY , 
     Type NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Message NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     StackTrace NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     WhatHappend NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     WhatHasBeenAffected NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     WhatActionsCanUserDo NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SupportInformation NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     CurrentUserID NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     MethodName NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     IPAddress NVARCHAR (15) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     PageURL NVARCHAR (300) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Browser NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     BrowserPlatform NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     BrowserCookies BIT , 
     BrowserJavaScript NVARCHAR (10) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ApplicationID SMALLINT NOT NULL DEFAULT (0) , 
     DeveloperID TINYINT , 
     Created DATETIME (8) NOT NULL DEFAULT getdate() 
    )
    ON [PRIMARY]
    TEXTIMAGE_ON 'PRIMARY' 
GO 



