CREATE TABLE dbo.Innovations 
    (
     InnovationID INTEGER NOT NULL IDENTITY , 
     InnovationParentID INTEGER , 
     InnovationTypeID INTEGER , 
     InventiveStepID INTEGER , 
     InventiveName NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     InventionDescriptionAsBarah NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     InventiveBriefExplanation NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     InventiveDetailsExplanation NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     IbtikarTypeName NVARCHAR (150) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     IbtikarTypeID INTEGER , 
     ImplementationIdeaName NVARCHAR (150) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ImplementationIdeaID INTEGER , 
     IdeaCategories INTEGER , 
     PatentOfficename NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     PatentOfficeID INTEGER , 
     Status INTEGER , 
     CreatedDate NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     CreateDate DATETIME (8) , 
     ApprovedDateAfterChecker DATETIME (8) , 
     CreatedUserID BIGINT , 
     LastUpdateUserID BIGINT , 
     LastUpdate NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UpdateDate DATETIME (8) DEFAULT getdate() , 
     NumberDeposit NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     URLForInventive NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ProblemSolvedByInventive NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     FeaturesBenefits NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     PatentNumberFromMadina NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     PatentImagePath NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     PatentIssueDate NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Year INTEGER , 
     ThereIsFileAttachement BIT , 
     ThereIsBarah BIT , 
     ThereIsProvideBarah BIT , 
     ProvideBarahImagePath NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     HavePreviousParticipatedInGalleries BIT , 
     PreviousParticipatedInGalleries NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ProvideDate NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ProvideOfficePatentID INTEGER , 
     ProvideOfficePatentName NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ThereIsContributers BIT , 
     ThereIsPrize BIT DEFAULT (0) , 
     ShowMyCv BIT DEFAULT (0) , 
     ConnectWithMe BIT , 
     CVNumber NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Deleted BIT DEFAULT (0) , 
     OldIdeaID INTEGER , 
     TotalTakhialEvaluationGrade INTEGER , 
     FromTakhial BIT DEFAULT (0) , 
     FromPatent BIT , 
     TransactionID BIGINT , 
     EducationalLogID INTEGER , 
     IbtikarGalleryUserType INTEGER , 
     CreatedDateRSD DATETIME (8) DEFAULT getdate() , 
     IsIdeaDev BIT DEFAULT (0) , 
     InventiveProblemSolvingBriefExplanation NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     IdeaDevIbtikarTypeID INTEGER , 
     IdeadevSourcesLookupID INTEGER , 
     IdeaDevStatusID INTEGER , 
     CheckerRecommendationID INTEGER , 
     IsRassedUpdated BIT 
    )
    ON [PRIMARY]
    TEXTIMAGE_ON 'PRIMARY' 
GO 



