    ON [PRIMARY]
    TEXTIMAGE_ON 'PRIMARY' 
GO 

    
    EXEC sp_addextendedproperty 'microsoft_database_tools_suppo', '1' , 
    'SCHEMA' , dbo, 'TABLE' , sysdiagrams
GO

ALTER TABLE dbo.sysdiagrams ADD CONSTRAINT PK__sysdiagrams__5DB5E0CB PRIMARY KEY CLUSTERED (diagram_id)
     WITH (
    FILLFACTOR = 80 , STATISTICS_NORECOMPUTE = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     ALLOW_ROW_LOCKS = ON )
     ON [PRIMARY]
GO
