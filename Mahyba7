CREATE TABLE dbo.Qiyas 
    (
     ID INTEGER NOT NULL IDENTITY , 
     UserID BIGINT , 
     ResultNo INTEGER , 
     ResultMessage NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Acc_No NVARCHAR (10) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     First_Name NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Middle_Name NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Grand_Name NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Last_Name NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ServiceInstancesID INTEGER , 
     Qstatus SMALLINT , 
     SMSSendDate DATETIME (8) 
    )
    ON [PRIMARY]
GO 



EXEC sp_addextendedproperty 'MS_Description' , 'Null=Service Not Call
0= Not Register & Not Paid
1=Registered & Not Paid
2=Register & Paid
' , 'SCHEMA' , 'dbo' , 'TABLE' , 'Qiyas' , 'COLUMN' , 'Qstatus' 
GO

    


CREATE UNIQUE NONCLUSTERED INDEX 
    IX_Qiyas ON dbo.Qiyas 
    ( 
     UserID 
    ) 
    WITH 
    ( 
     FILLFACTOR = 80 , 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
GO 


CREATE NONCLUSTERED INDEX 
    IX_Qiyas_UserID ON dbo.Qiyas 
    ( 
     UserID 
    ) 
    WITH 
    ( 
     FILLFACTOR = 80 , 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
