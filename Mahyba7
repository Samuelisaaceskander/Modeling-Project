CREATE TABLE dbo.MawhibaPrograms_Registration 
    (
     ID INTEGER NOT NULL IDENTITY , 
     UserID BIGINT , 
     MawhibaProgramsTypeID INTEGER , 
     AreaID INTEGER , 
     CityID INTEGER , 
     Mobile NVARCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Address NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ProgramYear INTEGER , 
     ZipCode NVARCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     PostBox NVARCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     HomePhone NVARCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     AreaIDForSchool INTEGER , 
     SchoolCityID INTEGER , 
     EducationRegionID INTEGER , 
     SchoolTypeID INTEGER , 
     SchoolName INTEGER , 
     SchoolClassID INTEGER , 
     SchoolPhone NVARCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SchoolFax NVARCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SchoolManagerName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SchoolManagerMobile NVARCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SchoolManagerEmail NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     GuardianName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     KinshipTypeID INTEGER , 
     GuardianJob NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     EducationLevelID INTEGER , 
     GuardianMobile NVARCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     HowKnowProgramsID INTEGER , 
     Other NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     CreationDate DATETIME (8) , 
     RegistrationPeriodID INTEGER , 
     TeacherName NVARCHAR (255) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     TeacherUserID BIGINT , 
     GuardianUserID BIGINT , 
     Status INTEGER , 
     EvaluationResult INTEGER , 
     IsSaudiMother BIT , 
     RegistrationLevel INTEGER DEFAULT (1) , 
     CreatorUserID BIGINT , 
     SMSsent BIT , 
     Rassed_TransactionID BIGINT , 
     EducationalRecordID BIGINT , 
     ParentEmail NVARCHAR (150) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     MotherNation INTEGER , 
     LastStatus INTEGER , 
     LastUpdate DATETIME (8) 
    )
    ON [PRIMARY]
GO 



