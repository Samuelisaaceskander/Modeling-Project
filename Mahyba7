     UserArabicFamilyName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     UserArabicSubtribeName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
