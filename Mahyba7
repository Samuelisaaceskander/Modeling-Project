CREATE NONCLUSTERED INDEX 
    IX_Innovations_Status_Deleted_InnovationTypeID ON dbo.Innovations 
    ( 
     Status , 
     Deleted , 
     InnovationTypeID 
    ) 
    WITH 
    ( 
     FILLFACTOR = 80 , 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
GO

ALTER TABLE dbo.Innovations ADD CONSTRAINT PK_InnovationVersion PRIMARY KEY CLUSTERED (InnovationID)
     WITH (
    FILLFACTOR = 70 , STATISTICS_NORECOMPUTE = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     ALLOW_ROW_LOCKS = ON )
     ON [PRIMARY]
GO

