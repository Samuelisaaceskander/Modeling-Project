CREATE TABLE dbo.UserBankAccount 
    (
     ID BIGINT NOT NULL IDENTITY , 
     UserID BIGINT NOT NULL , 
     BankID INTEGER , 
     BranchName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     OwnerName NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     IBAN NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     IsValid BIT , 
     Created DATETIME (8) NOT NULL DEFAULT getdate() , 
     ImageURL NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     IsCurrent BIT NOT NULL , 
     UpdatedBy BIGINT , 
     CreatedBy BIGINT , 
     Updated DATETIME (8) , 
     EducationalLevelID SMALLINT , 
     Employer NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     BankTypeID INTEGER DEFAULT (1) , 
     PassportNo NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     AccountNo NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     BankName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     BankAddress NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SwiftCode NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     BeneficiaryAddress NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     CountryID INTEGER , 
     BeneficiaryOwnerEnglishName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     FSUserTypeID INTEGER , 
     TitleTypeID INTEGER , 
     ABANumberSortCode NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS 
    )
    ON [PRIMARY]
GO 



