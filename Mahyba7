CREATE TABLE dbo.Programs 
    (
     ProgID INTEGER NOT NULL IDENTITY , 
     ArName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     NoOfStudents INTEGER , 
     GenderAudience CHAR (1) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     JStartDate DATETIME (8) NOT NULL , 
     JEndDate DATETIME (8) NOT NULL , 
     AgeRangeID SMALLINT , 
     CountryID INTEGER , 
     CityID INTEGER , 
     ManagerID BIGINT , 
     ContentManagerID BIGINT DEFAULT (1) , 
     ProgramSequenceID INTEGER , 
     SponsorID INTEGER , 
     Fees NUMERIC (8,2) , 
     SpecialityID SMALLINT , 
     PlaceID INTEGER , 
     ArAddress NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     URL NCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     TypeID INTEGER DEFAULT (1) , 
     LogoID INTEGER , 
     CurrentStatusID INTEGER DEFAULT (1) , 
     CreateDate SMALLDATETIME NOT NULL DEFAULT getdate() , 
     LastUpdate SMALLDATETIME NOT NULL DEFAULT getdate() 
    )
    ON [PRIMARY]
GO

