CREATE UNIQUE NONCLUSTERED INDEX 
    IX_LK_EducationRegions ON dbo.LK_EducationRegions 
    ( 
     ArabicName 
    ) 
    WITH 
    ( 
     FILLFACTOR = 70 , 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
GO 


CREATE NONCLUSTERED INDEX 
    IX_LK_EducationRegions_AreaID ON dbo.LK_EducationRegions 
    ( 
     AreaID 
    ) 
    WITH 
    ( 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
GO

