CREATE TABLE dbo.SCL_Schools 
    (
     ID INTEGER NOT NULL IDENTITY , 
     NoorID BIGINT NOT NULL , 
     SchoolStage NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     StudySystem NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SchoolMinistryNumber INTEGER , 
     SchoolName NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     AdministrativeRegionID INTEGER , 
     DistrictID INTEGER , 
     EducationOfficeID INTEGER , 
     StudyLevelDescriptionID INTEGER , 
     AdministrativeContryID INTEGER , 
     CitiesAndVillagesID NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SchoolEmail NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SchoolPhone NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SchoolGenderID INTEGER , 
     SchoolCategoryID INTEGER , 
     SchoolTime NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SchoolType NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     EducationType NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     WebsiteURL NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     tmp_NoorID NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     tmp_Name NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     tmp_Gender NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     tmp_schoolgender NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     tmp_disid NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     tmp_disname NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     tmp_studytype NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     MawhibaSchoolID INTEGER , 
     IsExistsAtNoor BIT NOT NULL DEFAULT (0) , 
     Created DATETIME (8) NOT NULL DEFAULT getdate() , 
     Updated DATETIME (8) , 
     IsDeleted BIT DEFAULT (0) 
    )
    ON [PRIMARY]
GO 

    


CREATE NONCLUSTERED INDEX 
    IX_SCL_Schools_DistrictID ON dbo.SCL_Schools 
    ( 
     DistrictID 
    ) 
    INCLUDE ( ID , NoorID , SchoolStage , StudySystem , SchoolMinistryNumber , SchoolName , AdministrativeRegionID , EducationOfficeID , StudyLevelDescriptionID , AdministrativeContryID , CitiesAndVillagesID , SchoolEmail , SchoolPhone , SchoolGenderID , SchoolCategoryID , SchoolTime , SchoolType , EducationType , WebsiteURL , tmp_NoorID , tmp_Name , tmp_Gender , tmp_schoolgender , tmp_disid , tmp_disname , tmp_studytype , MawhibaSchoolID , IsExistsAtNoor , Created , Updated , IsDeleted ) 
    WITH 
    ( 
     FILLFACTOR = 80 , 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
GO 


CREATE UNIQUE NONCLUSTERED INDEX 
    IX_SCL_Schools_NoorID ON dbo.SCL_Schools 
    ( 
     NoorID 
    ) 
    WITH 
    ( 
     FILLFACTOR = 70 , 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
GO 


CREATE NONCLUSTERED INDEX 
    IX_SCL_Schools_SchoolMinistryNumber ON dbo.SCL_Schools 
    ( 
     SchoolMinistryNumber 
    ) 
    WITH 
    ( 
     FILLFACTOR = 80 , 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
GO

