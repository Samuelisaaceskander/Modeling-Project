CREATE TABLE dbo.MSJ_Article 
    (
     ID INTEGER NOT NULL IDENTITY , 
     ArabicName NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     EnglishName NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     JournalID INTEGER NOT NULL , 
     IssueID INTEGER NOT NULL , 
     VolumeID INTEGER NOT NULL , 
     ArabicContent NTEXT COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     EnglishContent NTEXT COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     AutherID INTEGER NOT NULL , 
     ArticleImageURL NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     CreatedDate SMALLDATETIME DEFAULT getdate() , 
     ToView BIT NOT NULL DEFAULT (0) , 
     ArticleURL NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS 
    )
    ON [PRIMARY]
    TEXTIMAGE_ON 'PRIMARY' 
GO 

    


CREATE NONCLUSTERED INDEX 
    IX_MSJ_Article_VolumeID ON dbo.MSJ_Article 
    ( 
     VolumeID 
    ) 
    WITH 
    ( 
     FILLFACTOR = 80 , 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
GO

