     NafathNationalityCode INTEGER 
    )
    ON [PRIMARY]
GO 

    


CREATE UNIQUE NONCLUSTERED INDEX 
    Unique_LK_Nationalities_ArabicName ON dbo.LK_Nationalities 
    ( 
     ID 
    ) 
    INCLUDE ( ArabicName , EnglishName , CountryID , Rank ) 
    WITH 
    ( 
     FILLFACTOR = 80 , 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
GO 


