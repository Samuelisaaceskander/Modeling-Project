    [NonClusteredIndex-20170102-112801] ON dbo.UsersSocialInformation 
    ( 
     UserID 
    ) 
    INCLUDE ( UserGuardianName , UserGuardianPersonID , UserGuardianPersonIDPlaceOfIssuance , UserGuardianKinshipTypeID , UserGuardianWorkPhoneNumber , UserGuardianWorkFaxNumber , UserGuardianCellularPhoneNumber , UserGuardianEmail , UserGuardianJob , UserGuardianEducationalLevel , UserFatherEducationalLevelID , UserFatherJobTitle , UserMotherEducationalLevelID , UserMotherJobTitle , UserFamilyMembersCount , UserFamilyMemberOrder , userFamilyIncomeLevelID , userFamilyIncomeLevelvsOutComeID , UserGuardianPersonIDHijriExpirationDate , UserGuardianKinshipType , UserGuardianFatherName , UserGuardianGrandFatherName , UserGuardianFamilyName , Created ) 
    WITH 
    ( 
     IGNORE_DUP_KEY = OFF , 
     STATISTICS_NORECOMPUTE = ON , 
     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
