ALTER TABLE dbo.UserPersonalInformation ADD CONSTRAINT PK_UserPersonalInformation PRIMARY KEY CLUSTERED (UserID)
     WITH (
    FILLFACTOR = 70 , STATISTICS_NORECOMPUTE = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     ALLOW_ROW_LOCKS = ON )
     ON [PRIMARY]
GO

USE Mawhiba7
GO 

CREATE TABLE dbo.UserPersonalInformationCustomerCareLog 
    (
     UserID BIGINT NOT NULL , 
     UserName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     Password NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserMembershipCode BIGINT , 
     UserArabicFirstName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     UserArabicSecondName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     UserArabicThirdName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     UserArabicFamilyName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     UserArabicSubtribeName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserArabicFullName NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserEnglishFirstName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserEnglishSecondName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserEnglishThirdName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserEnglishFamilyName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserGender BIT , 
     UserHijriBirthdate NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserBirthDate DATETIME (8) , 
     UserBirthPlaceID INTEGER , 
     UserBirthPlace NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     IsAlive BIT , 
     VerificationType SMALLINT , 
     IsSaudiMother BIT , 
     UserPersonID NUMERIC (10) , 
     UserPersonIDPlaceOfIssuance NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserPersonIDHijriIssueDate NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserPersonIDHijriExpirationDate NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     YakeenValidation BIT , 
     IsMoqeem BIT , 
     ResidenceNumber NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ResidenceNumberIssuePlace NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ResidenceNumberIssueDate NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ResidenceNumberExpireDate NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserPassportNumber NVARCHAR (25) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserPassportExpireDate VARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     AccountTypeID TINYINT NOT NULL , 
     AccountActivation BIT NOT NULL , 
     ActivationBy TINYINT , 
     EmailActivation BIT , 
     SmsActivation BIT , 
     UserEmailAddress NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     UserCellularPhoneNumber NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserNationalityID INTEGER , 
     UserResidenceID INTEGER , 
     CityID INTEGER , 
     UserCity NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserMailBoxNumber NUMERIC (5) , 
     UserPostalCode NUMERIC (5) , 
     UserHomePhoneNumber NCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserFaxNumber NCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserWorkPhone NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserAgreeSendInformation BIT , 
     PersonalBlog NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ADGUID NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     CRMGuid NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     RequestUpdate BIT , 
     DeactivationReason NVARCHAR (2000) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ChatUserLevel VARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ChatUserLevelID INTEGER , 
     UserMailingAddress NTEXT COLLATE SQL_Latin1_General_CP1_CI_AS , 
     RequestRecoverPassword BIT , 
     CRM SMALLINT , 
     CreatedBy BIGINT , 
     CreateDate DATETIME (8) , 
     UpdatedBy BIGINT , 
     LastUpdateDate DATETIME (8) , 
     IsPasswordValidate BIT , 
     ActionType NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ActionBy BIGINT , 
     ActionDate DATETIME (8) DEFAULT getdate() 
    )
    ON [PRIMARY]
    TEXTIMAGE_ON 'PRIMARY' 
GO

USE Mawhiba7
GO 

CREATE TABLE dbo.UserPersonalInformationExtended 
    (
     ID BIGINT NOT NULL , 
     ADGUID NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Residence INTEGER , 
     ResidenceAreaID INTEGER , 
     ResidenceCities INTEGER , 
     ResidenceOtherCity NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserTypes NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserTypesOther NVARCHAR (150) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     AddressArea NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     AddressLine NVARCHAR (150) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     HouseNumber NVARCHAR (10) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     MailBox NVARCHAR (8) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ZipCode NVARCHAR (8) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     AlternateEmail NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     AlternateMobileNumber NCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     HomeTel NVARCHAR (15) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     WorkTel NVARCHAR (15) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     IsStudent BIT , 
     PersonalImageUrl NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     KidImageURL NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     About NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Updated DATETIME (8) , 
     IsCollaborator BIT DEFAULT (0) , 
     IsResidenceConfirm BIT DEFAULT (0) , 
     IAgent SMALLINT DEFAULT (1) , 
     HousingDistrictID INTEGER , 
     UserEmailAddressUpdateDate DATE , 
     UserCellularPhoneNumberUpdateDate DATE , 
     BannerImageUrl NVARCHAR (500) COLLATE SQL_Latin1_General_CP1_CI_AS 
    )
    ON [PRIMARY]
    TEXTIMAGE_ON 'PRIMARY' 
GO 



