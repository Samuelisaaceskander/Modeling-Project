CREATE TABLE dbo.SCL_Students 
    (
     ID INTEGER NOT NULL IDENTITY , 
     UserID BIGINT NOT NULL , 
     IdentificationID NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     NationalityID NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     IdentityExpirationDate NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     FirstName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     FatherName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     GrandfatherName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     FamilyName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Gender NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     BirthDate NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Religion NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SchoolName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SchoolMinistryID NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     StudySystemDescription NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ClassID NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SectionID NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     EnrollmentDescription NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     JoiningDate NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     IsExistsAtNoor BIT NOT NULL DEFAULT (0) , 
     Created DATETIME (8) NOT NULL DEFAULT getdate() , 
     Updated DATETIME (8) 
    )
    ON [PRIMARY]
GO 

    


