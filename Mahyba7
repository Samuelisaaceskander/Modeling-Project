     NewMobile NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     RequestGuid NVARCHAR (60) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserVerificationType INTEGER , 
