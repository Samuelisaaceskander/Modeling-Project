    (
     ProjectID INTEGER NOT NULL IDENTITY , 
     ProjectTrack CHAR (5) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     ProjectName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     ProjectEnglishName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ProjectDomainID INTEGER NOT NULL , 
     ProjectStartDate VARCHAR (10) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     ParticipatePreviously BIT NOT NULL , 
     ProjectRelated CHAR (6) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     TestExecutePlace CHAR (5) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ProjectCode VARCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ProjectType CHAR (5) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     SupervisorRegistrationID INTEGER NOT NULL , 
     FirstStudentRegistrationID INTEGER , 
     SecondStudentRegistrationID INTEGER , 
     [Order] INTEGER NOT NULL , 
     EducationalManagementAdminID INTEGER , 
     CreatedDate SMALLDATETIME NOT NULL DEFAULT getdate() , 
     YearID INTEGER , 
     IsCandidateToFinalShow BIT , 
     EducationRegionID INTEGER , 
