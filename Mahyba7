     EconomicTargetID INTEGER NOT NULL IDENTITY , 
     EconomicTargetName NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     EconomicTargetNameEnglish NVARCHAR (200) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Deleted BIT NOT NULL 
    )
    ON [PRIMARY]
GO

