     UserID BIGINT NOT NULL , 
     UserGuardianName NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserGuardianPersonID NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserGuardianPersonIDPlaceOfIssuance NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserGuardianKinshipTypeID TINYINT , 
     UserGuardianWorkPhoneNumber NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserGuardianWorkFaxNumber NCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserGuardianCellularPhoneNumber NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserGuardianEmail NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserGuardianJob INTEGER , 
     UserGuardianEducationalLevel INTEGER , 
     UserFatherEducationalLevelID SMALLINT , 
     UserFatherJobTitle NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserMotherEducationalLevelID SMALLINT , 
     UserMotherJobTitle NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserFamilyMembersCount SMALLINT , 
     UserFamilyMemberOrder SMALLINT , 
     userFamilyIncomeLevelID TINYINT , 
     userFamilyIncomeLevelvsOutComeID TINYINT , 
     UserGuardianPersonIDHijriExpirationDate NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserGuardianKinshipType NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserGuardianFatherName NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserGuardianGrandFatherName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserGuardianFamilyName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Created DATETIME (8) DEFAULT getdate() , 
     IsNonSaudiWithSaudiMother BIT , 
     UserGuardianHomePhoneNumber NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS 
    )
    ON [PRIMARY]
