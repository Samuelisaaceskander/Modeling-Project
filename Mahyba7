     SORT_IN_TEMPDB = OFF , 
     ALLOW_ROW_LOCKS = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     DROP_EXISTING = OFF , 
     ONLINE = OFF , 
     MAXDOP = 0 
    )
    ON [PRIMARY] 
