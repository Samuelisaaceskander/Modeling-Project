CREATE TABLE dbo.Inv_Inventions 
    (
     InventionID INTEGER NOT NULL IDENTITY , 
     InventionUsageID INTEGER , 
     Name NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     Problem NVARCHAR (4000) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Description NVARCHAR (4000) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Advantages NVARCHAR (4000) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Website VARCHAR (255) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     RejectionReason NVARCHAR (512) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     CreateUserID BIGINT NOT NULL , 
     CreateDate DATETIME (8) DEFAULT getdate() , 
     CurrentUserID BIGINT NOT NULL , 
     LastTransDate DATETIME (8) NOT NULL DEFAULT getdate() , 
     InventionStatusID INTEGER NOT NULL , 
     PatentNum NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     PatentSubmitDate NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     PatentIssuenceDate NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     PatentCategoryID INTEGER , 
     PatentOfficeID INTEGER , 
     ContactMobile NVARCHAR (4000) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ConcatEmail VARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ConcatName NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     OwnerName NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ByAdmin BIT , 
     PublishDate DATETIME (8) , 
     InventionField NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     participants NVARCHAR (3000) COLLATE SQL_Latin1_General_CP1_CI_AS 
    )
    ON [PRIMARY]
GO

ALTER TABLE dbo.Inv_Inventions ADD CONSTRAINT PK_Inventions PRIMARY KEY CLUSTERED (InventionID)
     WITH (
    FILLFACTOR = 80 , STATISTICS_NORECOMPUTE = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     ALLOW_ROW_LOCKS = ON )
     ON [PRIMARY]
GO

