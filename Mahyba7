     ArabicSummery NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     EnglishSummery NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ProjectPlan NVARCHAR (max) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ProjectDomainDetailID INTEGER , 
     IsExecuted BIT , 
     ExecuteHow CHAR (3) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     ExecuteHowOther NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SupervisorName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SupervisorMobile VARCHAR (20) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SupervisorEMail NVARCHAR (60) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     SupervisorNationalID VARCHAR (10) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     LastModifiedDate SMALLDATETIME , 
     ModifiedCount INTEGER DEFAULT (0) , 
     ProjectAccessDatabaseMembers INTEGER , 
     IsParticipateWithSchool BIT , 
     IsCandidateToRegion BIT , 
     IsParticipateWithFinalShow BIT DEFAULT (0) , 
