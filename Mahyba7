CREATE TABLE dbo.UserFamilyMembers 
    (
     ID INTEGER NOT NULL IDENTITY , 
     UserID BIGINT NOT NULL , 
     FullName NVARCHAR (350) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     KinshipID TINYINT NOT NULL , 
     Mobile NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     HomePhone NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Email NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     FaxNumber NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Qualification NCHAR (10) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Job NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     Employer NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     WorkPhone NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     IdentityNo CHAR (15) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     IsAlive BIT NOT NULL , 
     IsGuardian BIT NOT NULL , 
     IsContact BIT , 
     Created DATETIME (8) NOT NULL DEFAULT getdate() , 
     CreatedBy BIGINT NOT NULL , 
     Updated DATETIME (8) , 
     UpdatedBy BIGINT 
    )
    ON [PRIMARY]
GO 

    


