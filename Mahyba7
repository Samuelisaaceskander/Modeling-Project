     FamilyName NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     FullName NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     YakeenID NUMERIC (10) , 
     Type NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     OtherType NVARCHAR (50) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     EmailActivationCode NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     MobileActivationCode CHAR (10) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     UserID BIGINT , 
     Created SMALLDATETIME NOT NULL DEFAULT getdate() , 
     Updated DATETIME (8) DEFAULT getdate() , 
     IsCollaborator BIT , 
     IsEmailActivated BIT , 
     ServiceYouWishToRegister INTEGER , 
     RegistrationType INTEGER 
    )
    ON [PRIMARY]
GO 

    


