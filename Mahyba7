     ID INTEGER NOT NULL IDENTITY , 
     Name NVARCHAR (250) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL , 
     Description NVARCHAR (1000) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     IsActive BIT DEFAULT (0) , 
     OrganizationTypeID INTEGER NOT NULL , 
     Created SMALLDATETIME , 
     CreatedBy BIGINT , 
     Updated SMALLDATETIME , 
     UpdatedBy BIGINT 
    )
    ON [PRIMARY]
GO

ALTER TABLE dbo.Organizations ADD CONSTRAINT PK_Organizations_1 PRIMARY KEY CLUSTERED (ID)
     WITH (STATISTICS_NORECOMPUTE = ON , 
     ALLOW_PAGE_LOCKS = ON , 
     ALLOW_ROW_LOCKS = ON )
     ON [PRIMARY]
GO

