CREATE TRIGGER IbtikarInnovationsAddingInnovationsToBeReviewed ON dbo.IbtikarInnovations 
    AFTER INSERT 
    AS 
    CREATE TRIGGER [dbo].[IbtikarInnovationsAddingInnovationsToBeReviewed]
   ON  dbo.IbtikarInnovations
   after  INSERT
AS 
BEGIN
	-- SET NOCOUNT ON added to prevent extra result sets from
	-- interfering with SELECT statements.
	SET NOCOUNT ON;
	declare @innovationID int,@galleryId int,@Count int,@inv_id bigint
	select @innovationID=InnovationID,@galleryId=GalleryID,@inv_id=inv_id from inserted
	select @Count=count(*) from newInv where (Thereisbarah=1 or thereisask=1) and newinv.inv_id=@inv_id
	if(@Count=1)
	begin
	insert into dbo.IbtikarReviews 
		(
			InnovationID,GalleryID,Degree,[Status]
		)
		values
		(
			@innovationID,@galleryId,0,3
		)
	end
			-- Insert statements for trigger here
END
 
GO

CREATE TRIGGER tr_AfterUpdateInnovations ON dbo.Innovations 
    AFTER UPDATE 
    AS 
    -- =============================================
-- Author:		Elsayed Elbanna
-- Create date: 01/12/2016
-- Description:	update Rassed Status Column
-- =============================================
CREATE TRIGGER [dbo].[tr_AfterUpdateInnovations] 
   ON  [dbo].[Innovations]
   AFTER UPDATE
AS 
BEGIN
	-- SET NOCOUNT ON added to prevent extra result sets from
	SET NOCOUNT ON;
	declare @InnovationID int

	----------------Check update Fields for Rassed ---------------
	SELECT 
			@InnovationID =Inv.InnovationID
		FROM INSERTED Inv
			WHERE 
			Inv.InnovationTypeID in (2,4) 
			and Inv.IsIdeaDev=1 --Inv.Deleted =0 and  new update
			--and Inv.CreatedUserID not in (12721,69280)
	------------------------------------------------------------
       IF UPDATE(InventiveName) OR  UPDATE(ImplementationIdeaID) OR UPDATE(Status)  OR UPDATE(Deleted)
       BEGIN
				UPDATE Innovations
						SET IsRassedUpdated=0
				WHERE
						InnovationID =@InnovationID
       END
END
 
GO

CREATE TRIGGER UpdateStatusOnNewInv ON dbo.IbtikarReviews 
    AFTER UPDATE 
    AS 
    CREATE TRIGGER [dbo].[UpdateStatusOnNewInv] ON [dbo].[IbtikarReviews]
AFTER UPDATE 
AS
declare @st int
set @st=(select [Status] from inserted)
if @st=2
begin
update NewInv set inv_st=5 where inv_id in (select inv_id from dbo.IbtikarInnovations
						where InnovationID in (select InnovationID from IbtikarReviews
						where [Status]=2))
end
else
if @st=3
begin
update NewInv set inv_st=6 where inv_id in (select inv_id from dbo.IbtikarInnovations
						where InnovationID in (select InnovationID from IbtikarReviews
						where [Status]=3))
end
else
if @st=1
begin
update NewInv set inv_st=5 where inv_id in (select inv_id from dbo.IbtikarInnovations
						where InnovationID in (select InnovationID from IbtikarReviews
						where [Status]=1))
end
 
GO

CREATE TRIGGER UpdateStatusOnNewInvAfterArbitration ON dbo.IbtikarInnovations 
    AFTER UPDATE 
    AS 
    CREATE TRIGGER [dbo].[UpdateStatusOnNewInvAfterArbitration] ON dbo.IbtikarInnovations
AFTER UPDATE 
AS
declare @pass bit
declare @Status int
declare @InnovationID int
set @pass=(select Pass from inserted)
set @Status=(select Pass from inserted)
set @InnovationID=(select Pass from inserted)

if @pass=0
begin
update NewInv set inv_st=6 where inv_id in (select inv_id from dbo.IbtikarInnovations
						where Pass=0 and InnovationID=@InnovationID)
      and @Status=4
end
else
if @pass=1
update NewInv set inv_st=2 where inv_id in (select inv_id from dbo.IbtikarInnovations
						where Pass=1 and InnovationID=@InnovationID)
				and @Status=7

else
if @Status=8
begin
update NewInv set inv_st=3 where inv_id in (select inv_id from dbo.IbtikarInnovations
						where [Status]=8 and InnovationID=@InnovationID)
end
 
GO

CREATE TRIGGER UpdateStatusOnPatent_Innovations ON dbo.Patent_Reviews 
    AFTER INSERT 
    AS 
    CREATE TRIGGER [dbo].[UpdateStatusOnPatent_Innovations] ON [dbo].Patent_Reviews
AFTER insert 
AS
declare @st int

set @st=(select PatentInnovationID from inserted)

update Patent_Innovations set [Status]=8 where PatentInnovationID=@st 
GO

