CREATE TABLE dbo.Services 
    (
     ID INTEGER NOT NULL , 
     AliasName NVARCHAR (100) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     YearName CHAR (4) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     URL NVARCHAR (2000) COLLATE SQL_Latin1_General_CP1_CI_AS , 
     StartDate DATETIME (8) , 
     EndDate DATETIME (8) , 
     StartAge TINYINT , 
     EndAge TINYINT , 
     ParticipationCount TINYINT NOT NULL DEFAULT (1) , 
     EducationalYearID INTEGER , 
     UserGender TINYINT NOT NULL DEFAULT (2) , 
     IsUserVerificationRequired BIT NOT NULL DEFAULT (0) , 
     IsEmailValidationRequired BIT NOT NULL DEFAULT (0) , 
     IsMobileValidationRequired BIT NOT NULL DEFAULT (0) , 
     StatusID TINYINT NOT NULL DEFAULT (4) , 
     CategoryID INTEGER NOT NULL , 
     IsHistorical BIT NOT NULL DEFAULT (0) , 
     InstanceID INTEGER , 
     Created DATETIME (8) NOT NULL DEFAULT getdate() 
    )
    ON [PRIMARY]
GO 



